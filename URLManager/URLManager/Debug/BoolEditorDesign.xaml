<UserControl x:Class="URLManager.Debug.BoolEditorDesign"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:URLManager.Debug"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="300" Height="30">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="2*"/>
            <ColumnDefinition Width="3*"/>
        </Grid.ColumnDefinitions>
        <TextBlock Foreground="Black" VerticalAlignment="Center" HorizontalAlignment="Center" Text="ABCDEF"/>
        <CheckBox Margin="10,0" VerticalAlignment="Center" HorizontalAlignment="Left" Grid.Column="1"/>
    </Grid>
</UserControl>
